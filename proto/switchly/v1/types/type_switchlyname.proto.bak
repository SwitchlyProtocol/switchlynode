syntax = "proto3";
package types;

option go_package = "gitlab.com/switchly/switchlynode/v3/x/switchly/types";

import "common/common.proto";
import "gogoproto/gogo.proto";

message SWITCHNameAlias {
  string chain = 1 [(gogoproto.casttype) = "gitlab.com/switchly/switchlynode/v3/common.Chain"];
  string address = 2 [(gogoproto.casttype) = "gitlab.com/switchly/switchlynode/v3/common.Address"];
}

message SWITCHName {
  string name = 1 ;
  int64 expire_block_height = 2;
  bytes owner = 3  [(gogoproto.casttype) = "github.com/cosmos/cosmos-sdk/types.AccAddress"];
  common.Asset preferred_asset = 4 [(gogoproto.nullable) = false, (gogoproto.customtype) = "gitlab.com/switchly/switchlynode/v3/common.Asset"];
  repeated SWITCHNameAlias aliases = 5  [(gogoproto.nullable) = false];
}
