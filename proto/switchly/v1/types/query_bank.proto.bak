syntax = "proto3";
package types;

option go_package = "gitlab.com/switchly/switchlynode/v3/x/switchly/types";

import "gogoproto/gogo.proto";

option (gogoproto.marshaler_all) = true;

message QueryBalancesRequest{
  string address = 1;
  string height = 2;
}

message QueryBalancesResponse{
  repeated Amount balances = 1;
}

message Amount {
  string denom = 1;
  string amount = 2;
}