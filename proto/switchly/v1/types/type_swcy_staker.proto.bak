syntax = "proto3";
package types;

option go_package = "gitlab.com/switchly/switchlynode/v3/x/switchly/types";
option (gogoproto.goproto_getters_all) = false;

import "gogoproto/gogo.proto";
import "common/common.proto";

message SWCYStaker {
  string address = 1 [(gogoproto.casttype) = "gitlab.com/switchly/switchlynode/v3/common.Address"];
  string amount = 2 [(gogoproto.customtype) = "cosmossdk.io/math.Uint", (gogoproto.nullable) = false];
}
