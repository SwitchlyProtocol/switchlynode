syntax = "proto3";
package types;

option go_package = "gitlab.com/switchly/switchlynode/v3/x/switchly/types";

import "gogoproto/gogo.proto";

option (gogoproto.marshaler_all) = true;

message QuerySWCYStakerRequest{
  string address = 1;
  string height = 2;
}

// Can we replace this with SWCYStaker proto? Check json fields...
message QuerySWCYStakerResponse{
  string address = 1;
  string amount = 2 [(gogoproto.jsontag) = "amount"];
}

message QuerySWCYStakersRequest{
  string height = 1;
}

message QuerySWCYStakersResponse{
  repeated QuerySWCYStakerResponse tcy_stakers = 1;
}